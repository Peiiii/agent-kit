{
  "name": "@agent-labs/agent-chat",
  "version": "1.0.3",
  "packageManager": "pnpm@10.10.0",
  "description": "A React component library for building AI agent chat interfaces.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/agent-labs/agent-chat#readme",
  "bugs": {
    "url": "https://github.com/agent-labs/agent-chat/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agent-labs/agent-chat.git"
  },
  "author": "Agent Labs Team",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dist/index.css": "./dist/index.css",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "vite build",
    "dev": "vite build --watch",
    "playground": "vite --config playground/vite.config.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@ag-ui/client": "^0.0.28",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/vite": "^4.1.8",
    "@types/json-schema": "^7.0.15",
    "@types/react-syntax-highlighter": "^15.5.13",
    "json-schema": "^0.4.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0"
  },
  "prettier": "@sxzz/prettier-config",
  "yalcSig": "9d34700f90a222b05709a9c6a71cb28e"
}

    "happy-dom": "^17.4.6",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.8",
    "tsdown": "^0.11.1",
    "tsx": "^4.19.4",
